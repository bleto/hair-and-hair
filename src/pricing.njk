---
pagination:
  data: translations.locales
  size: 1
  alias: locale
permalink: "{% if locale == 'en' %}/en/pricing/{% else %}/pricing/{% endif %}"
layout: layout.njk
eleventyComputed:
  title: "{{ 'pricing.title' | t(locale) }}"
  description: "{{ 'pricing.description' | t(locale) }}"
---

<!--Page Header Start-->
<section class="page-header">
    <div class="page-header__bg" style="background-image: url(/assets/images/backgrounds/hah_5.jpg);">
    </div>
    <div class="container">
        <div class="page-header__inner">
            <ul class="thm-breadcrumb list-unstyled">
                <li>
                    <a href="{% if locale == 'en' %}/en/{% else %}/{% endif %}">{{ 'pricing.breadcrumb.home' | t(locale) }}</a>
                </li>
                <li><span>/</span></li>
                <li>{{ 'pricing.breadcrumb.current' | t(locale) }}</li>
            </ul>
            <h1 style="color: #fff">{{ 'pricing.title' | t(locale) }}</h1>
        </div>
    </div> 
</section>
<!--Page Header End-->


<!--Pricing Plans Start-->
<section class="pricing-plan">
    <div class="container">
        <div class="section-title text-center">
            <span class="section-title__tagline">{{ 'pricing.startingPacks.title' | t(locale) }}</span>
            <h2 class="section-title__title">{{ 'pricing.startingPacks.subtitle' | t(locale) }}</h2>
            <p class="section-title__text">{{ 'pricing.startingPacks.description' | t(locale) }}</p>
        </div>
        <div class="row">
            <!--Pricing Plan Single Start-->
            <div class="col-xl-4 col-lg-4 col-md-6 col-sm-12">
                <div class="pricing-plan__single">
                    <div class="pricing-plan__top">
                        <h3 class="pricing-plan__title">
                            {{ 'pricing.startingPacks.package1.name' | t(locale) }}
                        </h3>
                        <div class="pricing-plan__icon">
                            <span class="icon-spa-candles"></span>
                        </div>
                    </div>
                    <div class="pricing-plan__bottom">
                        <p class="pricing-plan__per-day">
                            <span class="pricing-plan__price pricing-plan__price--old">470 zł</span>
                            <span class="pricing-plan__price">400 zł</span><br> 
                            <span class="pricing-plan__time"><span class="fa fa-clock" aria-hidden="true"></span><span>120min</span></span>
                        </p>
                        <ul class="pricing-plan__points list-unstyled">
                            <li>
                                <div class="icon">
                                    <span class="fa fa-check"></span>
                                </div>
                                <div class="text">
                                    <p>{{ 'pricing.startingPacks.package1.option1' | t(locale) }}</p>
                                </div>
                            </li>
                            <li>
                                <div class="icon">
                                    <span class="fa fa-check"></span>
                                </div>
                                <div class="text">
                                    <p>{{ 'pricing.startingPacks.package1.option2' | t(locale) }}</p>
                                </div>
                            </li>
                            <li>
                                <div class="icon">
                                    <span class="fa fa-check"></span>
                                </div>
                                <div class="text">
                                    <p>{{ 'pricing.startingPacks.package1.option3' | t(locale) }}</p>
                                </div>
                            </li>
                            <li>
                                <div class="icon">
                                    <span class="fa fa-check"></span>
                                </div>
                                <div class="text">
                                    <p>{{ 'pricing.startingPacks.package1.option4' | t(locale) }}</p>
                                </div>
                            </li>
                            <li>
                                <div class="icon">
                                    <span class="fa fa-check"></span>
                                </div>
                                <div class="text">
                                    <p>{{ 'pricing.startingPacks.package1.option5' | t(locale) }}</p>
                                </div>
                            </li>
                        </ul>
                        <div class="pricing-plan__btn-box">
                            <a href="{% if locale == 'en' %}/en/booking/{% else %}/booking/{% endif %}" class="pricing-plan__btn thm-btn">
                                {{ 'pricing.startingPacks.cta' | t(locale) }}
