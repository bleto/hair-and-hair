<!--Services Two Start-->
<section class="services-two">
    <div class="container">
        <div class="services-two__inner">
            <div class="services-two__shape-1 float-bob-x">
                <img src="/assets/images/shapes/services-two-shape-1.png" alt="">
            </div>
            <div class="row">
                <!--Services Two Single Start-->
                <div class="col-xl-6 col-lg-6">
                    <div class="services-two__single">
                        <h3 class="services-two__title">{{ 'pricing.pricingConsultations.title' | t(locale) }}</h3>
                        <ul class="list-unstyled services-two__list">
                            {% for it in services.consultations %}
                            <li>
                                <div class="services-two__services-name">
                                    <h3><a href="{% if locale == 'en' %}/en/consultations/{% else %}/consultations/{% endif %}">{{ it.name | t(locale) }}</a></h3>
                                    <p>{{ it.duration }}</p>
                                </div>
                                <div class="services-two__services-price">
                                    <h4>{{ it.price }}</h4>
                                </div>
                            </li>
                            {% endfor %}
                        </ul>
                    </div>
                </div>
                <!--Services Two Single End-->
                <!--Services Two Single Start-->
                <div class="col-xl-6 col-lg-6">
                    <div class="services-two__single">
                        <h3 class="services-two__title">{{ 'pricing.pricingTreatments.title' | t(locale) }}</h3>
                        <ul class="list-unstyled services-two__list">
                            {% for it in services.treatments %}
                            <li>
                                <div class="services-two__services-name">
                                    <h3><a href="{% if locale == 'en' %}/en/treatments/{% else %}/treatments/{% endif %}">{{ it.name | t(locale) }}</a></h3>
                                    <p>{{ it.duration }}</p>
                                </div>
                                <div class="services-two__services-price">
                                    <h4>{{ it.price }}</h4>
                                </div>
                            </li>
                            {% endfor %}
                        </ul>
                    </div>
                </div>
                <!--Services Two Single End-->
            </div>
            <br>
            <div class="row">
                 <!--Services Two Single Start-->
                <div class="col-xl-6 col-lg-6 offset-xl-3 offset-lg-3">
                    <div class="services-two__single">
                        <h3 class="services-two__title">{{ 'pricing.headSPA.title' | t(locale) }}</h3>
                        <ul class="list-unstyled services-two__list">
                            {% for it in services.headSpa %}
                            <li>
                                <div class="services-two__services-name">
                                    <h3><a href="{% if locale == 'en' %}/en/head-spa/{% else %}/head-spa/{% endif %}">{{ it.name | t(locale) }}</a></h3>
                                    <p>{{ it.duration }}</p>
                                </div>
                                <div class="services-two__services-price">
                                    <h4>{{ it.price }}</h4>
                                </div>
                            </li>
                            {% endfor %}
                        </ul>
                    </div>
                </div>
                <!--Services Two Single End-->
            </div>
            <div class="row" style="margin-bottom:30px">
                <div class="col-xl-12">
                    <div class="services-two__btn-box">
                        <a href="{% if locale == 'en' %}/en/booking/{% else %}/booking/{% endif %}" class="thm-btn services-two__btn">
                            {{ 'pricing.bookNow' | t(locale) }}
                        </a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!--Services Two End-->


<!--Treatment Result Start-->
<section class="treatment-result">
    <div class="container">
        <div class="row">
            <div class="col-xl-6 col-lg-6">
                <div class="treatment-result__left">
                    <div class="section-title text-left">
                        <span class="section-title__tagline">{{ 'pricing.voucher.subtitle' | t(locale) }}</span>
                        <h2 class="section-title__title">{{ 'pricing.voucher.title' | t(locale) }}</h2>
                    </div>
                    <p class="treatment-result__text">{{ 'pricing.voucher.description' | t(locale) }}</p>
                    <div class="services-two__btn-box">
                        <a href="mailto:{{ site.email }}?subject={{ 'pricing.voucher.email.subject' | t(locale) | mailto }}&amp;body={{ 'pricing.voucher.email.body' | t(locale) | mailto }}" class="thm-btn services-two__btn">
                            {{ 'pricing.voucher.cta' | t(locale) }}
                        </a>
                    </div>
                </div>
            </div>
            <div class="col-xl-6 col-lg-6">
                <div class="treatment-result__right">
                    <div class="treatment-result__img-box">
                        <div class="treatment-result__img vouchers-image">
                            <img src="/assets/images/resources/vouchers.jpg" alt="{{ 'pricing.voucher.imageAlt' | t(locale) }}">
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!--Treatment Result End-->
