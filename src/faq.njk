---
pagination:
  data: translations.locales
  size: 1
  alias: locale
permalink: "{% if locale == 'en' %}/en/faq/{% else %}/faq/{% endif %}"
layout: layout.njk
eleventyComputed:
  title: "{{ 'faqPage.title' | t(locale) }}"
  description: "{{ 'faqPage.description' | t(locale) }}"
---

<!--Page Header Start-->
<section class="page-header">
  <div class="page-header__bg" style="background-image: url(/assets/images/backgrounds/hah_1.jpg);">
  </div>
  <div class="container">
    <div class="page-header__inner">
      <ul class="thm-breadcrumb list-unstyled">
        <li>
          <a href="{% if locale == 'en' %}/en/{% else %}/{% endif %}">{{ 'faqPage.breadcrumb.home' | t(locale) }}</a>
        </li>
        <li><span>/</span></li>
        <li>{{ 'faqPage.breadcrumb.current' | t(locale) }}</li>
      </ul>
      <h1 style="color: #fff">{{ 'faqPage.title' | t(locale) }}</h1>
    </div>
  </div>
</section>
<!--Page Header End-->

