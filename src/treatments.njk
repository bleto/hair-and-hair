---
pagination:
  data: translations.locales
  size: 1
  alias: locale
permalink: "{% if locale == 'en' %}/en/treatments/{% else %}/treatments/{% endif %}"
layout: layout.njk
eleventyComputed:
  title: "{{ 'treatments.title' | t(locale) }}"
  description: "{{ 'treatments.description' | t(locale) }}"
---

<!--Page Header Start-->
<section class="page-header">
    <div class="page-header__bg" style="background-image: url(/assets/images/backgrounds/treatments.jpg);">
    </div>
    <div class="container">
        <div class="page-header__inner">
            <ul class="thm-breadcrumb list-unstyled">
                <li>
                    <a href="{% if locale == 'en' %}/en/{% else %}/{% endif %}">{{ 'treatments.breadcrumb.home' | t(locale) }}</a>
                </li>
                <li><span>/</span></li>
                <li>{{ 'treatments.breadcrumb.current' | t(locale) }}</li>
            </ul>
            <h1 style="color: #fff">{{ 'treatments.title' | t(locale) }}</h1>
        </div>
    </div> 
</section>
<!--Page Header End-->

<script type="application/ld+json">
{
  "@context": "https://schema.org",
  "@type": "FAQPage",
  "mainEntity": [
    {
      "@type": "Question",
      "name": "{{ 'treatments.faq.q1' | t(locale) }}",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "{{ 'treatments.faq.a1' | t(locale) }}"
      }
    },
    {
      "@type": "Question",
      "name": "{{ 'treatments.faq.q2' | t(locale) }}",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "{{ 'treatments.faq.a2' | t(locale) }}"
      }
    },
    {
      "@type": "Question",
      "name": "{{ 'treatments.faq.q3' | t(locale) }}",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "{{ 'treatments.faq.a3' | t(locale) }}"
      }
    },
    {
      "@type": "Question",
      "name": "{{ 'treatments.faq.q4' | t(locale) }}",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "{{ 'treatments.faq.a4' | t(locale) }}"
      }
    }
  ]
}
</script>


