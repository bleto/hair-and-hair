                        <a class="instagram__link" href="{{ post.permalink }}" target="_blank" rel="noopener noreferrer" aria-label="Zobacz post na Instagramie">
                            <img src="{{ post.media_url }}" alt="{{ post.caption | truncate(50) }}" loading="lazy">
                            <div class="instagram__img-icon">
                                <span class="fab fa-instagram"></span>
                            </div>
                        </a>
                    </div>
                </div>
            </div>
            <!--Instagram Single End-->
            {% endfor %}
        </div>
    </div>
</section>
<!--Instagram End-->

