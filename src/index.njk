---
pagination:
  data: translations.locales
  size: 1
  alias: locale
permalink: "{% if locale == 'en' %}/en/{% else %}/{% endif %}"
layout: layout.njk
eleventyComputed:
  title: "{{ 'home.title' | t(locale) }}"
  description: "{{ 'home.description' | t(locale) }}"
---

<!--Main Slider Start-->
<section class="main-slider-two">
    <div class="swiper-container thm-swiper__slider" data-swiper-options='{"slidesPerView": 1, "loop": true,
"effect": "fade",
"pagination": {
"el": "#main-slider-pagination",
"type": "bullets",
"clickable": true
},
"navigation": {
"nextEl": "#main-slider__swiper-button-next",
"prevEl": "#main-slider__swiper-button-prev"
},
"autoplay": {
"delay": 5000
}}'>
        <div class="swiper-wrapper">

            <div class="swiper-slide">
                <div class="image-layer-two"
                    style="background-image: url(/assets/images/backgrounds/hah_1.jpg);"></div>
                <!-- /.image-layer -->

                <div class="container">
                    <div class="row">
                        <div class="col-xl-12">
                            <div class="main-slider-two__content">
                                <p class="main-slider-two__sub-title">{{ 'home.slider.slide1.subtitle' | t(locale) }}</p>
                                <h2 class="main-slider-two__title">{{ 'home.slider.slide1.title' | t(locale) | safe }}</h2>
                                <div class="main-slider-two__btn-box">
                                    <a href="{% if locale == 'en' %}/en/consultations/{% else %}/consultations{% endif %}" class="thm-btn main-slider-two__btn">{{ 'home.slider.slide1.button' | t(locale) }}</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="swiper-slide">
                <div class="image-layer-two"
                    style="background-image: url(/assets/images/backgrounds/hah_22.jpg);"></div>
                <!-- /.image-layer -->

                <div class="container">
                    <div class="row">
                        <div class="col-xl-12">
                            <div class="main-slider-two__content">
                                <p class="main-slider-two__sub-title">{{ 'home.slider.slide2.subtitle' | t(locale) }}</p>
                                <h2 class="main-slider-two__title">{{ 'home.slider.slide2.title' | t(locale) | safe }}</h2>
                                <div class="main-slider-two__btn-box">
                                    <a href="{% if locale == 'en' %}/en/treatments/{% else %}/treatments{% endif %}" class="thm-btn main-slider-two__btn">{{ 'home.slider.slide2.button' | t(locale) }}</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="swiper-slide">
                <div class="image-layer-two"
                    style="background-image: url(/assets/images/backgrounds/hah_3.jpg);"></div>
                <!-- /.image-layer -->

                <div class="container">
                    <div class="row">
                        <div class="col-xl-12">
                            <div class="main-slider-two__content">
                                <p class="main-slider-two__sub-title">{{ 'home.slider.slide3.subtitle' | t(locale) }}</p>
                                <h2 class="main-slider-two__title">{{ 'home.slider.slide3.title' | t(locale) | safe }}</h2>
                                <div class="main-slider-two__btn-box">
                                    <a href="{% if locale == 'en' %}/en/head-spa/{% else %}/head-spa{% endif %}" class="thm-btn main-slider-two__btn">{{ 'home.slider.slide3.button' | t(locale) }}</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="swiper-pagination" id="main-slider-pagination"></div>
        <!-- If we need navigation buttons -->


    </div>
</section>
<!--Main Slider End-->

<!--Our Intro Start-->
<section class="our-intro">
    <div class="our-intro__shape-1 float-bob-y">
        <img src="/assets/images/shapes/our-intro-shape-1.png" alt="Dekoracyjny element graficzny" role="presentation">
    </div>
    <div class="container">
        <div class="row">
            <div class="col-xl-6">
                <div class="our-intro__left">
                    <div class="our-intro__img-box wow slideInLeft" data-wow-delay="100ms"
                        data-wow-duration="2500ms">
                        <div class="our-intro__img">
                            <img src="/assets/images/resources/natalia_3.jpg" alt="{{ 'home.intro.imageAlt' | t(locale) }}" loading="lazy">
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-xl-6">
                <div class="our-intro__right">
                    <div class="section-title text-left">
                        <span class="section-title__tagline">{{ 'home.intro.tagline' | t(locale) }}</span>
                        <h2 class="section-title__title">{{ 'home.intro.title' | t(locale) }}</h2>
                    </div>
                    <p class="our-intro__text">
                        {{ 'home.intro.text' | t(locale) | safe }}
                    </p>
                    <div class="our-intro__points-and-spa-center">
                        <div class="our-intro__points-box">
                            <ul class="our-intro__points list-unstyled">
                                <li>
                                    <div class="icon">
                                        <span class="fa fa-check"></span>
                                    </div>
                                    <div class="text">
                                        <p>{{ 'home.intro.points.experience' | t(locale) }}</p>
                                    </div>
                                </li>
                                <li>
                                    <div class="icon">
                                        <span class="fa fa-check"></span>
                                    </div>
                                    <div class="text">
                                        <p>{{ 'home.intro.points.lecturer' | t(locale) }}</p>
                                    </div>
                                </li>
                                <li>
                                    <div class="icon">
                                        <span class="fa fa-check"></span>
                                    </div>
                                    <div class="text">
                                        <p>{{ 'home.intro.points.nursing' | t(locale) }}</p>
                                    </div>
                                </li>
                                <li>
                                    <div class="icon">
                                        <span class="fa fa-check"></span>
                                    </div>
                                    <div class="text">
                                        <p>{{ 'home.intro.points.collaboration' | t(locale) }}</p>
                                    </div>
                                </li>
                            </ul>
                            <div class="welcome-one__btn-box">
                                <a href="{% if locale == 'en' %}/en/consultations/{% else %}/consultations{% endif %}" class="welcome-one__btn thm-btn">{{ 'home.intro.button' | t(locale) }}</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!--Our Intro End-->

